{
    "extends": "eslint:recommended",
    "env": {
        "es6": true,
        "browser": true,
        "jasmine": true
    },
    "ecmaFeatures": {
        "modules": true
    },
    "rules": {
        "comma-dangle": [2, "always-multiline"],
        "no-alert": 2,
        "no-array-constructor": 2,
        "no-arrow-condition": 2,
        "no-caller": 2,
        "no-case-declarations": 2,
        "no-class-assign": 2,
        "no-const-assign": 2,
        "no-continue": 2,
        "no-dupe-class-members": 2,
        "no-empty-pattern": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-implied-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-extra-parens": 2,
        "no-floating-decimal": 2,
        "no-invalid-this": 2,
        "no-iterator": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-lonely-if": 2,
        "no-loop-func": 2,
        "no-mixed-requires": 2,
        "linebreak-style": [2, "unix"],
        "no-multi-spaces": 2,
        "no-multiple-empty-lines": [2, {"max": 2}],
        "no-native-reassign": 2,
        "no-nested-ternary": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-object": 2,
        "no-new-require": 2,
        "no-new-wrappers": 2,
        "no-octal-escape": 2,
        "no-path-concat": 2,
        "no-proto": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-shadow": 2,
        "no-shadow-restricted-names": 2,
        "no-sync": 2,
        "no-trailing-spaces": 2,
        "no-this-before-super": 2,
        "no-throw-literal": 2,
        "no-undef-init": 2,
        "no-undefined": 2,
        "no-unexpected-multiline": 2,
        "no-unneeded-ternary": 2,
        "no-unused-expressions": 2,
        "no-unused-vars": 2,
        "no-useless-concat": 2,
        "no-var": 2,
        "no-with": 2,

        "array-bracket-spacing": [2, "never"],
        "arrow-body-style": [2, "as-needed"],
        "arrow-parens": [2, "always"],
        "arrow-spacing": 2,
        "block-scoped-var": 2,
        "block-spacing": [2, "never"],
        "brace-style": [2, "stroustrup", { "allowSingleLine": true }],
        "callback-return": 2,
        "comma-spacing": [2, {"before": false, "after": true}],
        "comma-style": [2, "last"],
        "computed-property-spacing": [2, "never"],
        "constructor-super": 2,
        "curly": [2, "all"],
        "eol-last": 2,
        "eqeqeq": 2,
        "func-names": 2,
        "func-style": [2, "declaration"],
        "generator-star-spacing": [2, {"before": true, "after": false}],
        "handle-callback-err": 2,
        "key-spacing": [2, { "beforeColon": false, "afterColon": true }],
        "new-cap": 2,
        "new-parens": 2,
        "object-curly-spacing": [2, "never"],
        "object-shorthand": 2,
        "operator-linebreak":  [2, "after"],
        "prefer-arrow-callback": 2,
        "prefer-const": 2,
        "prefer-spread": 2,
        "prefer-template": 2,
        "quotes": [2, "single", "avoid-escape"],
        "semi": [2, "always"],
        "semi-spacing": 2,
        "space-after-keywords": 2,
        "space-before-blocks": 2,
        "space-before-function-paren": [2, { "anonymous": "always", "named": "never" }],
        "space-in-parens": [2, "never"],
        "space-infix-ops": 2,
        "space-return-throw-case": 2,
        "space-unary-ops": 2,
        "wrap-iife": 2
    },
    "globals": {
        "_": true,
        "exports": true,
        "localStorage": true,
        "inject": true,
        "process": true,
        "module": true,
        "moment": true,
        "require": true
    }
}
